{
  "views": {
      "SETTINGS": {
          "content_desc": "SETTINGS",
          "class": ".*FrameLayout"
      },
      "settings_house_edit_owners": {
          "text": "Edit House Owners",
          "class": ".*TextView"
      },
      "menu": {
          "resource_id": ".*house_activity_drawer_button_container"
      }
  },
  "states": {
      "setting_state": {
          "activity": ".*ui.main.house.HouseActivity",
          "views": [
              "SETTINGS", "menu"
          ]
      },
      "edit_owner_state": {
          "activity": ".*ui.main.house.HouseActivity",
          "views": [
              "settings_house_edit_owners"
          ]
      }
  },
  "operations": {
      "goto_settings": [
          {
              "event_type": "touch",
              "target_view": "SETTINGS"
          }
      ],
      "edit_house_owner": [
          {
              "event_type": "touch",
              "target_view": "settings_house_edit_owners"
          }
      ],
      "open_menu": [
          {
              "event_type": "touch",
              "target_view": "menu"
          }
      ]
  },
  "main": {
      "setting_state": [
          {
            "op_id": "goto_settings",
            "prob": 0.5
          },
          {
            "op_id": "open_menu",
            "prob": 0.1
          },
          {
            "op_id": "edit_house_owner",
            "prob": 0.1
         } 
      ]
  }
}